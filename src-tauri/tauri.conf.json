{
    "$schema": "https://schema.tauri.app/config/2",
    "build": {
        "beforeBuildCommand": "npm run build",
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:1420",
        "frontendDist": "../dist"
    },
    "bundle": {
        "active": false,
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.ico"
        ]
    },
    "productName": "redfish-schema-browser",
    "version": "0.2.0",
    "identifier": "9506hqwy.redfish.schema.browser",
    "mainBinaryName": "redfish-schema-browser",
    "plugins": {
        "cli": {
            "args": [
                {
                    "name": "schemadir",
                    "index": 1,
                    "required": false,
                    "takesValue": true
                }
            ]
        }
    },
    "app": {
        "windows": [
            {
                "title": "Redfish Schema Browser",
                "width": 1024,
                "height": 768,
                "fullscreen": false,
                "resizable": true,
                "maximizable": true
            }
        ],
        "security": {
            "csp": null
        }
    }
}
