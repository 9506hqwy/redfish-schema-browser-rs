{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": "",
    "beforeDevCommand": "",
    "devPath": "../dist",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "redfish-schema-browser",
    "version": "0.2.0"
  },
  "tauri": {
    "allowlist": {
      "dialog": {
        "open": true
      }
    },
    "bundle": {
      "active": false,
      "identifier": "9506hqwy.redfish.schema.browser",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "cli": {
      "args": [
        {
          "name": "schemadir",
          "index": 1,
          "required": false,
          "takesValue": true
        }
      ]
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 768,
        "resizable": true,
        "title": "Redfish Schema Browser",
        "width": 1024
      }
    ]
  }
}
